{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix './**/*.{ts,tsx}'",
    "format": "prettier --write",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "next": "11.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
